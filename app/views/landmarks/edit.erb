<form method="POST" action="/landmarks/<%= @landmark.id %>">
  <input id="hidden" type="hidden" name="_method" value="patch">
 <label>New Landmark<label>
 <input id="name" type="text" value="<%= @landmark.name %>" name="landmark[name]">
 <label>Year Completed</label>
 <input id="year_completed" type="text" value="<%= @landmark.year_completed %>" name="landmark[year_completed]">
 <input type="submit" value="Edit Landmark">
</form>
